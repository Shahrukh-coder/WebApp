@model Category

<form method="post">
    <div class="border p-3 mt-4">
        <div class="row pb-3"><h2>New Category</h2> <hr /></div>
        <div class="row mb-3"><label asp-for="Name"></label><input class="form-control" asp-for="Name" /></div>
        <div class="row mb-3"><label asp-for="DisplayOrder"></label><input class="form-control" asp-for="DisplayOrder" /></div>
        <button class="btn btn-primary" type="submit"> Create </button>

    </div>
    <a asp-action="Index" asp-controller="Category">Back To List</a>
</form>